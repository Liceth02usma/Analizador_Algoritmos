# Ejemplo 4: Suma de Matriz (English version)
# Complexity: O(nÂ²)

{
    int n = 10;
    int matrix[n][n];
    int sum = 0;
    int i, j;
    
    # Sum all elements in the matrix
    FOR i = 0 TO n-1 DO
        FOR j = 0 TO n-1 DO
            sum = sum + matrix[i][j];
        END_FOR
    END_FOR
    
    print("Total sum: " + sum);
}